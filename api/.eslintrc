{
  "parser": "babel-eslint",
  "extends": ["boldr"],
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "plugins": [
  ],
  "ecmaFeatures": {
    "defaultParams": true
  },
  "settings": {

  },
  "rules": {
    "no-unused-vars": 0,
    "babel/new-cap": 0,
    "babel/arrow-parens": 0,
    "import/no-unresolved": 0,
    "import/namespace": 0,
    "import/no-deprecated": 0,
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "import/default": 0,
    "react/prefer-stateless-function": 0
  },
  "globals": {
    "__DEVELOPMENT__": true,
    "__CLIENT__": true,
    "__SERVER__": true,
    "__DISABLE_SSR__": true,
    "__DEVTOOLS__": true,
    "__DLLS__": true,
    "socket": true,
    "webpackIsomorphicTools": true
  }
}
