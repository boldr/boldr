{
  "name": "boldr",
  "version": "0.7.0-alpha.2",
  "description": "universal react content management framework",
  "main": "boldrCMS/server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strues/boldr.git"
  },
  "keywords": [
    "react",
    "universal",
    "javascript",
    "express",
    "webpack",
    "postgres",
    "node",
    "cms",
    "redux"
  ],
  "author": "Steven Truesdell <steven@strues.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strues/boldr/issues"
  },
  "homepage": "https://github.com/strues/boldr#readme",
  "engines": {
    "node": ">=7.0.0",
    "npm": ">=3.0.0",
    "yarn": "^0.17.0"
  },
  "betterScripts": {
    "dev": {
      "command": "babel-node ./internal/development",
      "env": {
        "DEBUG": "boldr:*",
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    },
    "migrate": {
      "command": "knex migrate:latest",
      "env": {
        "POSTGRES_CONN_URI": "postgres://postgres:password@localhost:5432/boldr",
        "NODE_ENV": "development"
      }
    },
    "seed": {
      "command": "knex seed:run",
      "env": {
        "POSTGRES_CONN_URI": "postgres://postgres:password@localhost:5432/boldr",
        "NODE_ENV": "development"
      }
    }
  },
  "scripts": {
    "analyze": "babel-node internal/scripts/analyze",
    "babel": "NODE_ENV=production babel --ignore node_modules",
    "build": "babel-node internal/scripts/build",
    "clean": "babel-node internal/scripts/clean",
    "cleandb": "babel-node internal/scripts/destroyDB",
    "createdb": "babel-node internal/scripts/createDB",
    "dev": "better-npm-run dev",
    "development": "babel-node internal/development",
    "flow": "babel-node internal/scripts/flow",
    "flow:defs": "flow-typed install --overwrite",
    "flow:remove": "babel-node internal/scripts/removeFlow && rimraf tools/scripts/removeFlow.js",
    "lint": "eslint src",
    "knex": "./node_modules/.bin/knex",
    "migrate": "better-npm-run migrate",
    "seed": "better-npm-run seed",
    "start": "NODE_ENV=production node bin/boldr",
    "test:ci": "NODE_ENV=test CI=true jest -w 2 --coverage",
    "test": "jest",
    "test:watch": "NODE_ENV=test jest"
  },
  "jest": {
    "moduleNameMapper": {
      "^.*\\.scss$": "<rootDir>/internal/test/styleMock.js",
      "^.*\\.css$": "<rootDir>/internal/test/styleMock.js"
    },
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/(internal|config|docs|boldrCMS|.happypack|bin|.idea|public|db)/",
      "/flow-typed/",
      "/node_modules/",
      "<rootDir>/shared/theme/",
      "<rootDir>/shared/types/"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": ".*.test\\.js",
    "setupTestFrameworkScriptFile": "<rootDir>/internal/test/setup.js",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(internal|config|docs|boldrCMS|.happypack|bin|.idea|public|db)/",
      "__snapshots__"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "/.happypack/",
      "/boldrCMS/",
      "/db/"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "dependencies": {
    "app-root-dir": "1.0.2",
    "aws-sdk": "^2.7.27",
    "bcryptjs": "^2.4.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.0",
    "classnames": "^2.2.5",
    "colors": "1.1.2",
    "compression": "^1.6.2",
    "connect-redis": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "date-fns": "^1.27.1",
    "dotenv": "4.0.0",
    "draft-js": "^0.10.0",
    "draft-js-export-html": "^0.5.2",
    "draft-js-import-html": "^0.3.2",
    "express": "4.14.0",
    "express-session": "^1.15.0",
    "express-validator": "^3.1.2",
    "helmet": "3.4.0",
    "hpp": "^0.2.1",
    "humps": "^2.0.0",
    "immutability-helper": "^2.1.1",
    "invariant": "^2.2.2",
    "jsonwebtoken": "^7.2.1",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "method-override": "^2.3.7",
    "modernizr": "^3.3.1",
    "moment": "^2.17.1",
    "morgan": "^1.7.0",
    "multer": "^1.2.1",
    "multer-s3": "^2.5.0",
    "nodemailer": "^2.7.2",
    "normalizr": "3.1.0",
    "objection": "^0.6.2",
    "pg": "^6.1.2",
    "react": "15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "15.4.2",
    "react-helmet": "4.0.0",
    "react-md": "^1.0.1",
    "react-redux": "5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redial": "^0.5.0",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "serialize-javascript": "1.3.0",
    "source-map-support": "0.4.10",
    "styled-components": "^1.3.1",
    "superagent": "^3.3.2",
    "uuid": "^3.0.1",
    "webfontloader": "^1.6.27",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-cli": "6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-async-to-module-method": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.22.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-jsx-self": "^6.22.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "better-npm-run": "^0.0.14",
    "chai": "^3.5.0",
    "chokidar": "1.6.1",
    "css-loader": "0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "eslint": "3.14.0",
    "eslint-config-boldr": "0.7.2",
    "eslint-plugin-flowtype": "^2.30.0",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "faker": "^3.1.0",
    "file-loader": "0.9.0",
    "flow-bin": "0.38.0",
    "flow-remove-types": "1.1.2",
    "flow-typed": "2.0.0",
    "glob": "7.1.1",
    "happypack": "3.0.2",
    "jasmine-reporters": "^2.2.0",
    "jest-cli": "^18.1.0",
    "md5": "2.2.1",
    "modernizr-loader": "^1.0.1",
    "nock": "^9.0.2",
    "node-notifier": "4.6.1",
    "node-sass": "^4.3.0",
    "pgtools": "^0.2.3",
    "postcss-css-variables": "^0.6.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redbox-react": "^1.3.3",
    "redux-mock-store": "^1.2.1",
    "regenerator-runtime": "^0.10.1",
    "rimraf": "2.5.4",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "style-loader": "0.13.1",
    "stylelint": "^7.7.1",
    "supertest": "^3.0.0",
    "user-home": "^2.0.0",
    "webpack": "2.2.0",
    "webpack-bundle-analyzer": "2.2.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "1.5.4"
  }
}
