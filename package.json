    "eslint-loader": "^1.7.1",
    "extract-text-webpack-plugin": "2.1.0",
    "fast-async": "^6.2.2",
    "fast-sass-loader": "^1.2.4",
    "file-loader": "0.11.1",
    "flow-bin": "0.44.2",
    "flow-typed": "2.1.1",
    "glob": "7.1.1",
    "happypack": "^3.0.3",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-reports": "^1.0.2",
    "jest": "^19.0.2",
    "jest-enzyme": "^3.0.1",
    "lint-staged": "^3.4.0",
    "md5": "2.2.1",
    "moxios": "^0.4.0",
    "nock": "^9.0.13",
    "node-sass": "^4.5.2",
    "postcss": "^5.2.17",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
