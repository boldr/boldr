    "react-md": "^1.0.11",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-form": "^6.6.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0",
    "serialize-javascript": "1.3.0",
