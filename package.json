{
  "name": "boldr",
  "version": "0.0.0",
  "description": "universal react content management framework",
  "main": "boldrCMS/server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strues/boldr.git"
  },
  "keywords": [
    "react",
    "universal",
    "javascript",
    "express",
    "webpack",
    "postgres",
    "node",
    "cms",
    "redux"
  ],
  "author": "Steven Truesdell <steven@strues.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strues/boldr/issues"
  },
  "homepage": "https://github.com/strues/boldr#readme",
  "engines": {
    "node": ">=7.0.0",
    "npm": ">=3.0.0",
    "yarn": "^0.20.0"
  },
  "betterScripts": {
    "dev": {
      "command": "babel-node internal/development",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "boldr:*"
      }
    },
    "test": {
      "command": "jest -w 4 --config=internal/jest/jest.json",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "scripts": {
    "analyze": "babel-node internal/scripts/analyze --client",
    "babel": "NODE_ENV=production babel --ignore node_modules",
    "build:test": "babel-node internal/scripts/build",
    "build": "NODE_ENV=production babel-node internal/scripts/build --optimize",
    "clean": "babel-node internal/scripts/clean",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "better-npm-run dev",
    "flow:defs": "flow-typed install --overwrite",
    "flow": "babel-node internal/scripts/flow",
    "format": "prettier --single-quote=true --jsx-bracket-same-line=false --trailing-comma=all --write \"src/**/*.js\"",
    "lint": "eslint src",
    "nuke": "rimraf .happypack && rimraf boldrCMS && rimraf node_modules/.cache",
    "precommit": "lint-staged",
    "start": "node bin/boldr",
    "test:ci": "NODE_ENV=test jest -w 4 -u --config=internal/jest/jest.json --forceExit",
    "test": "better-npm-run test"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote=true --jsx-bracket-same-line=false --trailing-comma=all --write \"src/**/*.js\"",
      "git add"
    ]
  },
  "dependencies": {
    "animate-components": "^0.6.5",
    "app-root-dir": "1.0.2",
    "axios": "^0.16.1",
    "better-npm-run": "^0.0.15",
    "bluebird": "^3.5.0",
    "boldr-ui": "0.2.20",
    "boldr-utils": "0.2.0",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "date-fns": "^1.28.3",
    "debug": "^2.6.4",
    "dotenv": "4.0.0",
    "draft-js": "^0.10.0",
    "draft-js-import-html": "^0.3.2",
    "draftjs-to-html": "^0.7.1",
    "express": "4.15.2",
    "helmet": "3.5.0",
    "history": "^4.6.1",
    "hpp": "^0.2.2",
    "http-proxy": "^1.16.2",
    "immutability-helper": "^2.1.2",
    "invariant": "^2.2.2",
    "lodash": "^4.17.4",
    "normalizr": "3.2.2",
    "offline-plugin": "^4.7.0",
    "prop-types": "^15.5.8",
    "react": "15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "15.5.4",
    "react-draft-wysiwyg": "1.9.4",
    "react-dropzone": "3.13.0",
    "react-helmet": "5.0.3",
    "react-loadable": "^3.3.1",
    "react-redux": "5.0.4",
    "react-router-config": "1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "react-transition-group": "^1.1.2",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0",
    "response-time": "^2.3.2",
    "serialize-javascript": "1.3.0",
    "source-map-support": "0.4.14",
    "styled-components": "^1.4.5",
    "styled-theme": "^0.3.3",
    "uuid": "^3.0.1",
    "webfontloader": "^1.6.27"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "autoprefixer": "^6.7.7",
    "babel-cli": "6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "7.0.0",
    "babel-plugin-dynamic-import-node": "^1.0.1",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
