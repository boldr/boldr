  "engines": {
    "node": ">=7.0.0",
    "npm": ">=3.0.0",
    "yarn": ">=0.23.0"
  },
  "homepage": "https://github.com/strues/boldr#readme",
  "keywords": [
    "cms",
    "express",
    "javascript",
    "node",
    "postgres",
    "react",
    "redux",
    "universal",
    "webpack"
  ],
  "license": "MIT",
  "lint-staged": {
    "src/**/*.js": [
      "prettier --single-quote=true --jsx-bracket-same-line=false --trailing-comma=all --write \"src/**/*.js\"",
      "git add"
    ]
  },
  "main": "boldrCMS/server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strues/boldr.git"
  },
  "scripts": {
    "analyze": "babel-node internal/scripts/analyze --client",
    "babel": "NODE_ENV=production babel --ignore node_modules",
    "build": "NODE_ENV=production USE_PROXY=false babel-node --max_old_space_size=4096 internal/scripts/build --optimize",
    "build:proxy": "NODE_ENV=production USE_PROXY=true babel-node internal/scripts/build --optimize",
    "build:test": "babel-node internal/scripts/build",
    "clean": "babel-node internal/scripts/clean",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "better-npm-run dev",
    "flow": "babel-node internal/scripts/flow",
    "flow:defs": "flow-typed install --overwrite",
    "format": "prettier --single-quote=true --jsx-bracket-same-line=false --trailing-comma=all --write \"src/**/*.js\"",
    "lint": "eslint src",
    "nuke": "rimraf .happypack && rimraf boldrCMS && rimraf node_modules/.cache && rimraf .cache-loader",
    "precommit": "lint-staged",
    "prepush": "yarn test",
    "start": "NODE_ENV=production node boldrCMS/server/index.js",
    "test": "better-npm-run test",
    "test:ci": "NODE_ENV=test jest -w 4 -u --config=internal/jest/jest.json --forceExit"
  },
  "jest": {
    "timers": "fake",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internal/jest/fileMock.js"
    },
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/(internal|server|config|docs|boldrCMS|.happypack|bin|.idea|public|db)/",
      "/flow-typed/",
      "/__fixtures__/",
      "/node_modules/",
      "<rootDir>/shared/styles/",
      "<rootDir>/shared/types/"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/internal/jest/transform.js"
    },
    "testRegex": ".*.test\\.js",
    "setupTestFrameworkScriptFile": "<rootDir>/internal/jest/setup.js",
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(server|internal|config|docs|boldrCMS|.happypack|bin|.idea|public|db)/",
      "__snapshots__",
      "/styles/"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "/.happypack/",
      "/boldrCMS/",
      "/db/",
      "/styles/"
    ],
    "coverageReporters": [
      "json"
    ]
  }
}
