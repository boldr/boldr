{
  "name": "boldr-frontend",
  "description": "Boldr frontend",
  "version": "0.1.0",
  "main": "server/index.js",
  "license": "MIT",
  "author": {
    "name": "Steven Truesdell",
    "email": "steven@strues.io",
    "url": "strues.io"
  },
  "scripts": {
    "build": "NODE_ENV=production boldr build",
    "build:client": "NODE_ENV=production webpack --progress -p --config webpack/client.prod.js",
    "build:server": "rimraf buildServer && cross-env NODE_ENV=production webpack --progress -p --config webpack/server.prod.js",
    "build:flow": "find ./src -name '*.js' -not -path '*/__tests__*' | while read filepath; do cp $filepath `echo $filepath | sed 's/\\/src\\//\\/lib\\//g'`.flow; done",
    "build-storybook": "NODE_ENV=production BABEL_ENV=sb:production build-storybook",
    "cm": "git-cz",
    "init": "boldr init tmp",
    "clean": "boldr clean all",
    "dev": "NODE_ENV=development boldr dev",
    "migr": "boldr migrate",
    "lint": "eslint --fix src server webpack",
    "start": "npm run clean && cross-env NODE_ENV=development GRAPHQL_ENDPOINT=http://localhost:2121/api/v1/graphql node server/index.js",
    "start:prod": "NODE_ENV=production boldr start:ssr",
    "storybook": "NODE_ENV=story start-storybook -p 6006",
    "precommit": "lint-staged --verbose",
    "prettier": "node ./internal/scripts/prettier.js write",
    "test": "NODE_ENV=test jest --config jest.config.json"
  },
  "dependencies": {
    "@boldr/core": "0.1.0-beta.3",
    "@boldr/config": "^0.1.2",
    "@boldr/icons": "0.1.0-rc.2",
    "@boldr/tools": "^0.1.0-beta.4",
    "@boldr/ui": "0.1.8-alpha.2",
    "@boldr/utils": "0.1.1",
    "bootstrap": "4.0.0-beta",
    "date-fns": "^1.29.0",
    "draft-convert": "^2.0.0",
    "draft-js": "^0.10.3",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "ramda": "0.25.0",
    "raf": "3.4.0",
    "react": "^16.0.0",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-popper": "0.7.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-tagsinput": "^3.18.0",
    "react-transition-group": "^2.2.1",
    "react-universal-component": "2.5.5",
    "recompact": "^3.2.0",
    "reactstrap": "5.0.0-alpha.3",
    "redux": "^3.7.2",
    "redux-form": "^7.1.1",
    "redux-thunk": "^2.2.0",
    "reselect": "3.0.1",
    "serialize-javascript": "^1.4.0",
    "styled-components": "^2.2.1",
    "webpack-flush-chunks": "^1.2.0"
  },
  "devDependencies": {
    "@boldr/cli": "0.2.0-beta.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-boldr": "0.11.2",
    "cross-env": "^5.0.5",
    "express": "^4.16.2",
    "flow-bin": "^0.57.2",
    "jest-styled-components": "4.7.0",
    "prettier": "^1.7.4",
    "react-hot-loader": "next",
    "react-test-renderer": "^16.0.0",
    "redbox-react": "^1.5.0",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2"
  }
}
