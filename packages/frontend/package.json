{
  "name": "boldr-frontend",
  "description": "Boldr frontend",
  "version": "0.1.0",
  "main": "server/index.js",
  "license": "MIT",
  "author": {
    "name": "Steven Truesdell",
    "email": "steven@strues.io",
    "url": "strues.io"
  },
  "scripts": {
    "build": "NODE_ENV=production boldr build",
    "build:client": "NODE_ENV=production webpack --progress -p --config webpack/client.prod.js",
    "build:server": "rimraf buildServer && cross-env NODE_ENV=production webpack --progress -p --config webpack/server.prod.js",
    "build:flow": "find ./src -name '*.js' -not -path '*/__tests__*' | while read filepath; do cp $filepath `echo $filepath | sed 's/\\/src\\//\\/lib\\//g'`.flow; done",
    "build-storybook": "NODE_ENV=production BABEL_ENV=sb:production build-storybook",
    "cm": "git-cz",
    "clean": "boldr clean all",
    "dev": "NODE_ENV=development boldr dev",
    "migr": "boldr migrate",
    "lint": "eslint --fix src server webpack",
    "start": "npm run clean && cross-env NODE_ENV=development GRAPHQL_ENDPOINT=http://localhost:2121/api/v1/graphql node server/index.js",
    "start:prod": "NODE_ENV=production boldr start:ssr",
    "storybook": "NODE_ENV=story start-storybook -p 6006",
    "precommit": "lint-staged --verbose",
    "prettier": "node ./internal/scripts/prettier.js write",
    "test": "NODE_ENV=test jest --config jest.config.json"
  },
  "dependencies": {
    "@boldr/core": "0.1.0-beta.2",
    "@boldr/config": "^0.1.0-alpha.1",
    "@boldr/icons": "0.1.0-rc.2",
    "@boldr/tools": "^0.1.0-beta.2",
    "@boldr/ui": "0.1.5",
    "@boldr/utils": "^0.1.0-rc.5",
    "color": "2.0.0",
    "date-fns": "^1.28.5",
    "draft-convert": "^2.0.0",
    "draft-js": "^0.10.1",
    "immutable": "^3.8.1",
    "immutability-helper": "2.3.1",
    "lodash": "^4.17.4",
    "polished": "1.7.0",
    "ramda": "0.24.1",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-motion": "^0.5.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-tagsinput": "^3.17.0",
    "react-transition-group": "^2.2.0",
    "react-universal-component": "2.5.4",
    "recompact": "^3.1.0",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-thunk": "^2.2.0",
    "reselect": "3.0.1",
    "prop-types-extra": "^1.0.1",
    "rc-tabs": "^9.1.4",
    "react-visibility-sensor": "^3.10.1",
    "serialize-javascript": "^1.4.0",
    "semantic-ui-react": "^0.73.0",
    "styled-components": "^2.1.2",
    "webpack-flush-chunks": "^1.1.23"
  },
  "devDependencies": {
    "@boldr/cli": "0.2.0-beta.1",
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-centered": "^3.2.7",
    "@storybook/addon-info": "^3.2.9",
    "@storybook/addon-knobs": "^3.2.8",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/addon-options": "^3.2.6",
    "@storybook/react": "^3.2.8",
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-preset-boldr": "0.11.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.6.1",
    "eslint-config-boldr": "^0.12.13",
    "express": "^4.15.4",
    "flow-bin": "^0.54.1",
    "jest-styled-components": "4.6.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.7.0",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.3.0",
    "redbox-react": "^1.5.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6"
  }
}
