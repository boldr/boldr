{
  "env": {
    "es": {
      "presets": [
        [
          "boldr/node",
          {
            "useBuiltins": true,
            "modules": false,
            "exclude": [
              "transform-regenerator",
              "transform-async-to-generator"
            ]
          }
        ]
      ]
    },
    "common": {
      "presets": [
        [
          "boldr/node",
          {
            "useBuiltins": true,
            "modules": "commonjs",
            "exclude": [
              "transform-regenerator",
              "transform-async-to-generator"
            ]
          }
        ]
      ]
    }
  }
}
