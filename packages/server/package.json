{
  "name": "@boldr/server",
  "version": "0.0.1-alpha.1",
  "description": "Server for Boldr",
  "main": "lib/index.js",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "build": "rm -rf lib && NODE_ENV=production babel src --out-dir lib",
    "dev": "NODE_ENV=development DEBUG=boldr:* nodemon",
    "dev:root": "NODE_ENV=development DEBUG=boldr:* babel-node src/index.js",
    "migrate": "DATABASE_URL=postgres://postgres:password@localhost:5432/boldr node --harmony internal/scripts/db.js migrate",
    "migration": "DATABASE_URL=postgres://postgres:password@localhost:5432/boldr node --harmony internal/scripts/db.js migration",
    "seed": "DATABASE_URL=postgres://postgres:password@localhost:5432/boldr node --harmony internal/scripts/db.js seed",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "babel-node internal/scripts/updateSchema.js"
  },
  "keywords": [
    "graphql",
    "apollo",
    "express",
    "objection",
    "node"
  ],
  "author": {
    "name": "Steven Truesdell",
    "email": "steven@strues.io",
    "url": "strues.io"
  },
  "license": "MIT",
  "homepage": "https://github.com/strues/boldr#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strues/boldr.git"
  },
  "bugs": {
    "url": "https://github.com/strues/boldr/issues"
  },
  "dependencies": {
    "@boldr/config": "^0.1.0-alpha.1",
    "@boldr/utils": "^0.1.0-rc.5",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.1",
    "connect-redis": "^3.3.0",
    "convict": "^4.0.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "date-fns": "^1.28.5",
    "debug": "^3.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "formidable": "^1.1.1",
    "fs-extra": "^4.0.1",
    "graphql": "^0.11.3",
    "graphql-server-express": "^1.1.2",
    "graphql-subscriptions": "^0.4.4",
    "helmet": "^3.8.1",
    "hpp": "^0.2.2",
    "ioredis": "^3.1.4",
    "jimp": "^0.2.28",
    "jsonwebtoken": "^8.0.1",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "merge-graphql-schemas": "^1.1.3",
    "morgan": "^1.8.2",
    "nodemailer": "^4.1.0",
    "object-path": "^0.11.4",
    "objection": "^0.8.9",
    "pg": "^7.3.0",
    "pretty-error": "^2.1.1",
    "shortid": "^2.2.8",
    "subscriptions-transport-ws": "^0.8.3",
    "uuid": "^3.1.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.6.1",
    "eslint-config-boldr": "^0.12.13",
    "fast-async": "^6.3.0",
    "mockdate": "^2.0.2",
    "nodemon": "^1.12.0",
    "prettier": "1.6.1"
  }
}
