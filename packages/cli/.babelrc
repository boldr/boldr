{
  "presets": [
    [
      "env",
      {
        "targets": { "node": "7" },
        "modules": "commonjs",
        "exclude": ["transform-regenerator", "transform-async-to-generator"]
      }
    ]
  ],
  "plugins": [
    [
      "fast-async",
      {
        "spec": true
      }
    ],
    "transform-object-rest-spread"
  ]
}
